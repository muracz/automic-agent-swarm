
[GLOBAL]
name=#AGENT#
system=#SYSTEM#
language=(E,D)
logging=/dev/stdout
logCount=10
helplib=ucx.msl
killSignal=SIGKILL
license_Class=V
login_Check=no
open_File_Max=32768
readUserAlways=no
reportMode=600
uc_User_Type=EXCL
userid_Type=EXCL
fileProcessingMaxDepth=0
fileProcessingTimeout=0
ft_Owner=user
ft_Temp_File=yes
jobFileMode=700
ServiceProgramStartCommand=su %USER% -c '%IOSCOMMAND%'
listenerUID=65534
listenerGID=65534

[TRACE]
file=../temp/ucxjxxx_t##.txt
trccount=10
tcp/ip=0
event=0
ex_init=0
ft_debug=0
job_debug=0
mail=0
memory=0
signal=0

[TCP/IP]
cp=#CP#
connect=60
maxMsgSize=500000
maxRepCnt=8
port=2300
sendBufferSize=1024k
recvBufferSize=1024k
tcp_KeepAlive=Y
tcp_KeepAlive_Time=6600
tcp_Nodelay=1

[AUTHORIZATION]
initialPackage=
keystore=

[FILETRANSFER]
ft_Check_Free_Disk_Space=no
ft_Linkfiles=no

[HOSTS]

[MISC]
authentication=local
FileBufferSize=0
FileEndDelimiter=no
fileRemoveCheck=yes
MsgToStdout=no
processInfo=yes
TraceFileSize=32M

[PAM]
libName=/usr/lib64/libpam.so.0
;pam_Open_Session=no

[STARTCMD]
start_Type=fork

[USERID]
root=START

[UC_USER]

[VARIABLES]
UC_EX_JOB_MD=ucxjlx6m
UC_EX_PATH_BACKUP=../backup/
UC_EX_PATH_BIN=.
UC_EX_PATH_JOBREPORT=../temp
UC_EX_PATH_TEMP=../temp
UC_HOST_CODE=UC_CODE
UC_HOST_JCL_VAR=UNIX

[CP_LIST]
